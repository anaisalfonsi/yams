extends layouts/main.pug 

block content
    h1 Hello #{user.firstName}
    ul
        each dice in dices ? dices : []
            li= dice.number
    div 
        a(href="/play") Jouer
    div
        p #{message}

    ul
        each pastry in user.pastries
            li= pastry.name

    p #{user.pastriesCount}